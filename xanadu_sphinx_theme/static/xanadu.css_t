}

.method>dt::before {
  content: 'meth ';
  font-style: italic;
}

.attribute>dt::before {
  content: 'attr ';
  font-style: italic;
}

.class>dt {
  background: #eee;
  border-radius: 5px;
  padding: 5px;
}

.class>dt code {
