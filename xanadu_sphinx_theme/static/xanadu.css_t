    height: 100%;
}

.plugin-card:hover {
    transition-duration: 0.4s;
    box-shadow: 0 2px 5px 0 rgba(0, 0, 0, .05), 0 2px 25px 2px rgba(0, 0, 0, .15) !important;
}

